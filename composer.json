{
    "name": "luchavez/gitlab-toolkit",
    "description": "Gitlab Toolkit for Laravel 8|9|10",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "jamescarloluchavez@gmail.com",
            "homepage": "https://www.linkedin.com/in/jsluchavez/"
        }
    ],
    "homepage": "https://github.com/luchavez-technologies/gitlab-toolkit",
    "keywords": ["Laravel", "GitlabToolkit"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~8|~9|~10",
        "luchavez/gitlab-sdk": "^1.0",
        "luchavez/boilerplate-generator": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~6|~7|~8"
    },
    "autoload": {
        "psr-4": {
            "Luchavez\\GitlabToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luchavez\\GitlabToolkit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luchavez\\GitlabToolkit\\Providers\\GitlabToolkitServiceProvider"
            ],
            "aliases": {
                "GitlabToolkit": "Luchavez\\GitlabToolkit\\Facades\\GitlabToolkit"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.0.x-dev"
        }
    }
}
