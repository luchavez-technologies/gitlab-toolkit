{
    "name": "luchavez/fligno-toolkit",
    "description": "Fligno Toolkit Laravel Package",
    "license": "MIT",
    "authors": [
        {
            "name": "James Carlo Luchavez",
            "email": "jamescarloluchavez@gmail.com",
            "homepage": "https://www.linkedin.com/in/jsluchavez/"
        },
        {
            "name": "Fligno",
            "email": "apps@fligno.com",
            "homepage": "https://fligno.com/"
        }
    ],
    "homepage": "https://github.com/luchmewep/fligno-toolkit",
    "keywords": ["Laravel", "FlignoToolkit"],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~8|~9",
        "luchavez/gitlab-sdk": "^1.0",
        "luchavez/boilerplate-generator": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~6|~7"
    },
    "autoload": {
        "psr-4": {
            "Luchavez\\FlignoToolkit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luchavez\\FlignoToolkit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luchavez\\FlignoToolkit\\Providers\\FlignoToolkitServiceProvider"
            ],
            "aliases": {
                "FlignoToolkit": "Luchavez\\FlignoToolkit\\Facades\\FlignoToolkit"
            }
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev",
            "dev-develop": "1.0.x-dev"
        }
    }
}
